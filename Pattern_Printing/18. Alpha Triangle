#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter n: ";
    cin>>n;
    for(int i=1;i<=n;i++){
        int c=(n-i)+'A';
        char t=c;
        for(int j=1;j<=i;j++){
            cout<<t<<" ";
            t++;
        }
        c--;
        cout<<endl;
    }
    return 0;
}

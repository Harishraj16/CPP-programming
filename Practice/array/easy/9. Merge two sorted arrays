#include<bits/stdc++.h>
vector < int > sortedArray(vector < int > a, vector < int > b) {
    set<int> st;
    for(int i=0;i<a.size();i++){
        st.insert(a[i]);
    }

    for(int i=0;i<b.size();i++){
        st.insert(b[i]);
    }

    vector<int> res;
    for(auto x:st){
        res.push_back(x);
    }

    return res;
}
